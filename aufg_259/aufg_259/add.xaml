<Window x:Class="aufg_259.add"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:aufg_259"
        mc:Ignorable="d"
        Title="add" Height="200 " Width="200">
    <StackPanel Margin="10">
        <Label>Name der Währung:</Label>
        <TextBox x:Name="waehrung_add" Text="" />
        <Label>Umrechnungsfaktor(1 Euro) </Label>
        <TextBox x:Name="ufaktor_add"></TextBox>
        <Button x:Name="Add_Waehrung_Button"  Margin="3,10" Click="Add_Waehrung_Button_Click" Content="Neue Währung hinzufügen"></Button>
    </StackPanel>
</Window>
